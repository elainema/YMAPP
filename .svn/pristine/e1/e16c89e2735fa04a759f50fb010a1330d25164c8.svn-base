/*login*/
img{
	display: none; 
}
.mui-bar-nav{
	box-shadow: none;
}
.mui-bar-nav~.mui-content{
	padding-top: 50px;
	padding-bottom: 50px;
}
.mui-table-view-cell.mui-collapse .mui-active img{
	display: none;
}
.mui-table-view-cell.mui-collapse .mui-active .mui-navigate-right:after,
.mui-table-view-cell.mui-collapse.mui-active>.mui-navigate-right:after, .mui-table-view-cell.mui-collapse.mui-active>.mui-push-right:after{
	content:'';
}
.mui-icon-reload{
	color: #4c4c4c;
}
.mui-table-view-cell.mui-collapse .mui-active .mui-navigate-right:after,
.mui-table-view-cell.mui-collapse.mui-active>.mui-navigate-right:after, .mui-table-view-cell.mui-collapse.mui-active>.mui-push-right:after{
	content:'';
	color:transparent;
	border: none;
	border-radius: 1em;	
}
.spread{
	float: right;
	margin-top:-1em;
}
.mui-navigate-right:after{
	font-size: 1.5em;
}
#messageContainer img{
	display: block;
}
#msgDeatilContent img{
	display: inline-block;
}
#login{
	background: #35393f;
	color:#d1d1d2 !important;
}
#login h1{
	padding: 14.3% 0 8%;
}
#loginForm{
	width: 75%;
	margin: auto;
}
#loginForm .form-group{
	border-bottom: 1px solid #909090;
	margin-bottom: 1em;
}
.mui-checkbox input[type=checkbox]{
	top:0;
} 
.mui-checkbox input[type=checkbox]:before, .mui-radio input[type=radio]:before{
	text-align: center;
	border: 1px solid #909090;
	color:transparent;
	content: "\e472"; 
	width: 1em;
	height: 1em;
	/*border-radius: 0.625em;*/
	border-radius:0.3125em;
	font-size:1em;
}
.mui-radio input[type=radio]{
	top:0.5em;
}
.mui-checkbox input[type=checkbox]:checked:before, .mui-radio input[type=radio]:checked:before{
	text-align: center;
	border: 1px solid #f37c25;
	color: #f37c25;
	content: "\e472";
	width: 1em;
	height: 1em;
	border-radius: 1.25em;
	border-radius:0.3125em;
	font-size:1em;
}
.mui-checkbox{
	margin: auto;
}
#loginForm label, input[type=text], input[type=password]{
	font-family: "Roboto thin";
	height: auto;
	padding: 0;
	background-color: transparent;
	border:none;
	width: 90%;
	font-size: 0.9375em;
	margin-top: 0.5em;
	margin-bottom: 0.5em;
}
input[type=text], input[type=password]{
	margin-left: 13%;
	padding-right: 15%;
}
input[type=text]:focus, input[type=password]:focus{
	border:none;
} 
.icon{
	position: absolute;
	top: 0;
	left: 0.2em;
}
#loginForm label{
	width: auto;
	padding-right: 1.875em;
	margin-left: 0.3em;
}
#commit{
	font-family: "Roboto Regular";
	font-size: 1em;
}
.mui-checkbox input[type=checkbox]{
	float:none;
	position: relative;
}
.mui-checkbox {
	margin: auto;
	margin-bottom: 10px;
}
.mui-btn-positive,
.mui-btn-positive:active{
	background-color: #2a2d34;
	border: 1px solid #2e2a34;
}
::-webkit-input-placeholder{
    color: #717478;
}
/*setting*/
.mui-bar-nav{
	height: 2.75em;
	height: 8%;
	height: 50px;
	border-bottom: 1px solid #cdcdcd;
	background: #f3f3f3;
background: -moz-linear-gradient(top,  #f3f3f3 0%, #f0f0f0 100%);
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#f3f3f3), color-stop(100%,#f0f0f0));
