	font-size: 12px;
}
.mui-bar-tab .mui-tab-item{
	border-top: 0.3em solid transparent;
	position: relative;
	height: 50px;
}
.mui-bar-tab .mui-tab-item.mui-active{
	color: #ffffff;
	border-top: 0.3em solid #ff8023;
	background: #2a2d34;
	height: 50px;
}

