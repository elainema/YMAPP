	font-family: "Roboto-Medium";
	font-size: 1.25em;
	color:#4c4c4c;
	margin-top: 0;
}
.mui-bar-tab{
	background: #34393f;
	box-shadow: none;
	border-top: 0;
}
.mui-bar-tab p{
	margin-bottom: -0.8em;
}
.mui-bar-tab .mui-tab-item .mui-icon{
	width: auto;
	height: auto;
}
